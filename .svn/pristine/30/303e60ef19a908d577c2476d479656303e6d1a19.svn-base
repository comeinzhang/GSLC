@charset "utf-8";
/* CSS Document */
body{ margin: 0 auto; font-size:15px; margin:0 auto; overflow-x:hidden; font-family:Arial, Helvetica, sans-serif, 微软雅黑; background-color:#f3f2f7; color:#323232;}
/*单独搜索页*/
.searchou .qxlogo{ width:11%; height:45px; background:url(../images/kxzLogo.png) center no-repeat; background-size:21px; float:left;}
