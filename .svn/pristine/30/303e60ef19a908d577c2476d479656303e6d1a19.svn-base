.search_cl{ color:#ccac6a;}
.search_r p a{ line-height:40px; color:#323232; width:100%; display:block; background: url(../img/searchr.png) right no-repeat #FFF; padding-left:20px; border-bottom:1px #e4e4e4 solid;}
.search_show{display:none;}
.search_l p a{ line-height:40px; color:#323232; width:100%; display:block; background:#FFF; padding-left:20px; border-bottom:1px #e4e4e4 solid;}
.search_l p span{ display:block; width:40px; height:40px; background:url(../img/searchx.png) left no-repeat; float:right;}
/*banner*/
.banner{min-width:320px; max-width:750px; margin:0 auto; overflow:hidden; margin-top:45px; }
.slider{display:none;}/*用于获取更加体验*/
.focus span{width:10px;height:10px;margin-right:10px;border-radius:50%; font-size:10;}
.focus span.current{ background:#ccac6a;}
/*nav*/
nav { background: #ffffff; padding: 10px 0; margin-bottom: 10px; border-bottom: 1px #E5E5E7 solid; }
nav a{ width: 25%; float: left; display: block; text-align: center; color:#323232; }
nav a img{ width: 40px; margin-bottom: 5px; }

/*分类菜单*/
.index-menu{ background: #ffffff;}
.index-menu-left{ width: 55%;float: left; }
