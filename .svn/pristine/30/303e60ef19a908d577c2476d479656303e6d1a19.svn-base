.search{ width:100%; height:100%; background:#f3f2f7; z-index:10; position:fixed; top:46px; display:block; display:none;}
.searchou{ width:100%; height:45px; background:#f9f9f9; z-index: 9999;  border-bottom: 1px #e4e4e6 solid;}
