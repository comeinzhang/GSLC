@charset "UTF-8";
.select-btn{ width: 40px; height: 90px; position: absolute; left: 0; top: 10px; z-index: 1 }
.select-btn p{ width: 15px; height: 15px; position: absolute; left: 50%; margin-left: -7px;
    top: 50%; margin-top: -7px; background: url("../../../images/sel_btn.png") 0 -15px no-repeat; background-size: 15px;  }
.select-btn .active{ background: url("../../../images/sel_btn.png") 0 0 no-repeat; background-size: 15px;  }
.gwc-index{ margin-top: 5px; margin-bottom: 100px;}
.gwc-index .g-list .goods-box .content .pic{ height: 90px; width: 90px; left: 0; top: 10px; z-index: 4;}
.gwc-index .g-list .goods-box .content .text{ height: 90px; margin-left: 90px;}


.gwc-index .g-list .goods-box .info .d1{  width: auto;}
.gwc-index .g-list .goods-box .content .text .msg-gg{ height: auto; font-size: 12px; margin-bottom: 0; }
.gwc-index .g-list .goods-box .content .text .name{ margin-bottom: 0; padding-bottom: 0; }
.gwc-index .g-list .goods-box .content{ border-bottom: 1px #EFEFEF solid; }
.gwc-index .g-list .add-num{ height: 28px; }
.gwc-index .g-list .add-num p{ float: left;}
.gwc-index .g-list .add-num .p1{ width: 28px; line-height: 28px; margin-right: 15px;}
.gwc-index .g-list .add-num .p2{ height: 28px; width: 28px; text-align: center; line-height: 30px; background: #f9f9f9;}
.gwc-index .g-list .add-num .p3{ height: 28px; width: 28px; border-bottom: 1px #d7d7d7 solid; margin: 0 5px;}
.gwc-index .g-list .add-num .p3 input{ width: 100%; height: 27px; text-align: center; line-height: 1; border: 0; background: 0; font-size: 16px; }
.gwc-index .g-list .text .pc{ position: absolute; right: 0;  bottom: 2px; text-align: right; }
.gwc-index .g-list .text .pc .p1{ color: #8e8e93; text-decoration: line-through; font-size: 13px; }
.gwc-index .g-list .text .pc .p2{ color: #fe6345; font-size: 16px; font-weight: 700;}
.gwc-index .g-list .info-list .dsl .select-btn{ width: 20px; height: 40px; left: -5px; top: 1px; }
.gwc-index .g-list .info-list .dsl .dp{ margin-left: 20px;}
.gwc-index .g-list .info-list .dsl .dp .icon-more{ font-size: 12px; position: relative; top: -2px; }
.gwc-index .g-list .goods-box .content .del-btn{  background: #f8351b; color: #ffffff; line-height: 110px; position: absolute; right: 0; top: 0;
 width: 60px;  text-align: center; z-index: 1;}
.gwc-index .g-list .goods-box .content .g-add{ position: relative; z-index: 2; background: #fff; padding: 10px; margin-left: 40px;}
.gwc-index .g-list .goods-box .content{ padding: 0;}
.gwc-index .gwc-bottom{ width: 100%; height: 45px; position: fixed; left: 0; bottom: 50px; border-bottom: 1px #EFEFEF solid;
background: #ffffff; z-index: 9;}
.gwc-index .gwc-bottom .show1{ position: relative; height: 45px;}
.gwc-index .gwc-bottom .show1 .js{ line-height: 45px; padding-left: 15px; }
.gwc-index .gwc-bottom .show1 .right-btn{ height: 45px; position: absolute; right: 0; top: 0; background: #f8351b; width: 120px; line-height: 45px; text-align: center;
color: #ffffff; }
.gwc-index .gwc-bottom .show2{ height: 45px; position: relative;}
.gwc-index .gwc-bottom .show2 .d1{ height: 45px; position: relative; width: 80px;}
.gwc-index .gwc-bottom .show2 .d1 .select-btn{  height: 45px; left: 0; top: 0; }
.gwc-index .gwc-bottom .show2 .d1 .sp{ margin-left: 35px; line-height: 45px; }
.gwc-index .gwc-bottom .show2 .show2-btn{ position: absolute; height: 45px; width: 180px; right: 0; top: 0; }
.gwc-index .gwc-bottom .show2 .show2-btn .btn{ position: absolute; padding: 0 10px; line-height: 25px; border: 1px #f8351b solid;
 top: 9px; color: #f8351b; border-radius: 8px; text-align: center;}
.gwc-index .gwc-bottom .show2 .show2-btn .sc-btn{}
.gwc-index .gwc-bottom .show2 .show2-btn .del-btn{ right: 15px;}














