}

.countdownCpt .box .time > li.num:after {
  content: ' : ';
  position: absolute;
  right: 0px;
  top: 0;
}

.countdownCpt .box .time > li.num.noAfter:after {
  content: '';
}

.countdownCpt .box .time > li:not(.num) {
