*{margin: 0; padding: 0;}
body{background-color:#f3f3f3; color:#333; font-family: "PingFang SC", "Lantinghei SC", "Microsoft YaHei", "HanHei SC", "Helvetica Neue", "Open Sans", Arial, "Hiragino Sans GB", "Microsoft Yahei", "STHeiti", "WenQuanYi Micro Hei", SimSun, sans-serif,"\5B8B\4F53";
 font-size: 14px; }
img, input, select { vertical-align: middle; }
h1, h2, h3, h4, h5, h6 { font-weight:normal; font-size:1em; }
ol, ul { list-style-type:none; }
table { border-collapse:collapse; border-spacing:0; }
th,caption { text-align:left; }
a img { border:0 none; cursor:pointer; }
fieldset,img { border:0 none; }
a, input { outline:0 none; }
a:link, a:visited { text-decoration:none; }
a, div, p, span, li, input{-webkit-tap-highlight-color: rgba(0,0,0,0);  -webkit-focus-ring-color: rgba(0, 0, 0, 0); -moz-tap-highlight-color: rgba(0,0,0,0); -moz-focus-ring-color: rgba(0, 0, 0, 0);  }
a{ color:#333;}
a:hover{ /*text-decoration:underline; */ }

.container{ width:1200px;margin:0 auto;}
.container-1000{ width:1000px;margin:0 auto;}
.clearFix:after{    content: "020";    display: block;    height: 0;    clear: both;    visibility: hidden;}
.clearFix { /* 触发 hasLayout */    zoom: 1;}
.ellipsis{ white-space:nowrap;text-overflow:ellipsis;overflow:hidden;-webkit-text-overflow:ellipsis;}
.none{ display: none;}

