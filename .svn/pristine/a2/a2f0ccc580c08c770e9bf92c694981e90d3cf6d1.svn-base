.l-list .row{ width: 50%; float: left; background: #FFFFFF;  }
.l-list .row a{ display: block; padding-bottom: 10px; overflow: hidden;border-left: 1px #d7d7d7 solid;border-top: 1px #d7d7d7 solid;}
.l-list .row .name{ white-space:nowrap;text-overflow:ellipsis;overflow:hidden;-webkit-text-overflow:ellipsis; padding: 5px 10px;}
.l-list .row .pic{ padding-bottom: 100%; height: 0; overflow: hidden; position: relative;}
.l-list .row .pic img{ width: 100%; position: absolute; left: 0; top: 0;}
.l-list .row .text{ margin: 0 10px; position: relative;}
.l-list .row .text .num{ position: absolute; right: 0; top: 0; color: #999999; }
.l-list .row .text .price span{ color: #ff115f; }

.t-banner{ margin-top: 45px;}
.index_slider{  margin: 10px; overflow: hidden; }
.index_slider .slide{ overflow: inherit;  }
.index_slider .slide:after{ padding-top:27%;  }
.index_slider .slide .dot{ bottom: 5px;  }
.index_slider .slide .dot span{
    display: inline-block;
    width: 5px;
    height: 5px;
    margin-left: 5px;
    background-color: #a9b8c6;
    border-radius: 50%;
    border: none;
}
.index_slider .slide .dot .cur{
    background-color: #08335d;
}