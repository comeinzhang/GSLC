.button a{ display: block; color: #ffffff; }
.up-sfz{ padding-bottom: 20px; margin-top: 10px;  }
.up-sfz .row{  width: 50% ;float: left; margin-bottom: 10px; text-align: center; position: relative;}
.up-sfz .row p{  margin-top: 10px; }
.up-sfz .row .uf{ margin: 0 10px; height: 130px; background: #ffffff; overflow: hidden; position: relative; }
.up-sfz .row img{ height: 130px; position: absolute; left: 50%;
    transform: translate(-50%,0);
    -ms-transform: translate(-50%,0); /* IE 9 */
    -webkit-transform: translate(-50%,0); }
.up-sfz .up-btn{ display: block; height: 100%; width: 100%; z-index: 2;
    position: absolute; left: 0;top: 0; opacity: 0;}

